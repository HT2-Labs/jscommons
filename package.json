    "@types/cors": "2.8.3",
    "@types/express": "4.11.1",
    "@types/fs-extra": "5.0.0",
    "@types/helmet": "0.0.37",
    "@types/knex": "0.14.7",
    "@types/lodash": "4.14.103",
    "@types/mocha": "2.2.48",
    "@types/moment": "2.13.0",
