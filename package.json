    "file-stream-rotator": "^0.2.0",
    "fs-extra": "^5.0.0",
    "helmet": "^3.6.1",
    "knex": "^0.14.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongodb": "^3.0.1",
    "morgan": "^1.8.2",
    "newrelic": "^2.3.1",
    "rulr": "^4.0.1",
    "uuid": "^3.1.0",
    "winston": "^2.3.1",
    "winston-aws-cloudwatch": "^2.0.0"
