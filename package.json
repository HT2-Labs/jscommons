  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/HT2-Labs/jscommons.git"
  },
  "files": [
    "dist",
    "package-lock.json",
    "tsconfig.json",
    "tslint.json"
  ],
  "scripts": {
    "acp": "git add -A && npm run commit && git push",
